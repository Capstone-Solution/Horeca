<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="0">

    <record id="seq_loan_gg_id" model="ir.sequence">
      <field name="name">loans loans</field>
      <field name="code">loans.loane</field>
      <field name="prefix">LO/%(y)s/%(month)s/</field>
      <field name="padding">3</field>
      <field name="company_id" eval="False"/>
    </record>

    <record id="seq_violations_id" model="ir.sequence">
      <field name="name">violations</field>
      <field name="code">violations</field>
      <field name="prefix">VIO/%(y)s/%(month)s/</field>
      <field name="padding">3</field>
      <field name="company_id" eval="False"/>
    </record>

    <record id="seq_salary_id" model="ir.sequence">
      <field name="name">salary advance</field>
      <field name="code">salary.advance</field>
      <field name="prefix">SA/%(y)s/%(month)s/</field>
      <field name="padding">3</field>
      <field name="company_id" eval="False"/>
    </record>
  </data>
</odoo>